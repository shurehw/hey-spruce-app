{
  "name": "hey-spruce-mobile",
  "version": "1.0.0",
  "description": "Hey Spruce Mobile Apps for Clients and Subcontractors",
  "main": "index.js",
  "scripts": {
    "start": "ionic serve",
    "build": "ionic build",
    "build:client": "ionic build --project=client",
    "build:subcontractor": "ionic build --project=subcontractor",
    "sync": "ionic capacitor sync",
    "sync:ios": "ionic capacitor sync ios",
    "sync:android": "ionic capacitor sync android",
    "client:serve": "ionic serve --project=client",
    "subcontractor:serve": "ionic serve --project=subcontractor",
    "client:build": "ionic build --project=client && ionic capacitor sync",
    "subcontractor:build": "ionic build --project=subcontractor && ionic capacitor sync"
  },
  "dependencies": {
    "@ionic/angular": "^7.0.0",
    "@ionic/capacitor-app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/push-notifications": "^5.0.0",
    "@capacitor/camera": "^5.0.0",
    "@capacitor/geolocation": "^5.0.0",
    "@capacitor/local-notifications": "^5.0.0",
    "@capacitor/filesystem": "^5.0.0",
    "angular": "^16.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@ionic/cli": "^7.0.0"
  }
}